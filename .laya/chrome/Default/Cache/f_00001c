;\n    } else {\n        throw new Error('params error');\n    }\n};\nexport {stop, resume, viewport};\nexport default gen;\n\n\n// WEBPACK FOOTER //\n// ./node_modules/@super-fe/BdrainfnScroll/src/index.js","/**\n * @file react Uilib\n * @author lironghui@baidu.com\n */\n\nimport split from './split.css';\nimport font from './font.css';\nimport distence from './distence.css';\nimport img from './img.css';\nimport otherUi from './otherUi.css';\nimport height from './height.css';\nimport onepx from './onepx.css';\n\nconst param = {\n    H: [0, 19, 36],\n    M: [0, 17, 14, 12, 8, 1, 7, 5, 10, 20, 6],\n    P: {small: (3 / 2), large: (16 / 9)},\n    T: [\n        {\n\n        },\n        {\n            fontSize: 18,\n            color: '#333'\n        },\n        {\n            fontSize: 13,\n            color: '#999'\n        },\n        {\n            fontSize: 16,\n            color: '#666'\n        },\n        {\n            fontSize: 12,\n            color: '#666'\n        },\n        {\n            fontSize: 18,\n            color: '#999'\n        }\n    ]\n};\n\nconst Uilib = {split, font, distence, img, otherUi, height, param, onepx};\n\nexport default Uilib;\n\n\n\n// WEBPACK FOOTER //\n// ./node_modules/@super-fe/BdrainrwUilib/dist/index.js","var store = require('./_shared')('wks');\nvar uid = require('./_uid');\nvar Symbol = require('./_global').Symbol;\nvar USE_SYMBOL = typeof Symbol == 'function';\n\nvar $exports = module.exports = function (name) {\n  return store[name] || (store[name] =\n    USE_SYMBOL && Symbol[name] || (USE_SYMBOL ? Symbol : uid)('Symbol.' + name));\n};\n\n$exports.store = store;\n\n\n\n//////////////////\n// WEBPACK FOOTER\n// ./node_modules/core-js/modules/_wks.js\n// module id = 3\n// module chunks = 0","/*\n\tMIT License http://www.opensource.org/licenses/mit-license.php\n\tAuthor Tobias Koppers @sokra\n*/\n// css base code, injected by the css-loader\nmodule.exports = function(useSourceMap) {\n\tvar list = [];\n\n\t// return the li