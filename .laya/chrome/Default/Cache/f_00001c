meout(this.timeout);\n            this.timeout = null;\n        }\n    }\n\n    /**\n     * \n     * @param {Object} data - \n     * @param {number} data.status - error code\n     */\n    report(data) {\n        this.statistics();\n        this.clear();\n        this.fire(data);\n    }\n\n    defaultFn(report) {\n        report({status: this.opts.base + this.step});\n    }\n\n    next(fn = this.defaultFn, timeout = 5000) {\n        if (this.finished) {\n            return;\n        }\n        this.clear();\n        this.statistics();\n        this.step++;\n        this.timeout = setTimeout(() => fn.call(this, this.report), timeout);\n    }\n\n    detectData(getState, opts = {}) {\n        this.next(report => {\n            let store = isFunction(getState) ? getState() : getState;\n            let data = result(store, this.path);\n            if (isEmpty(data)) {\n                report(Detect.STATUS.NOT_UPDATE);\n            } else {\n                this.defaultFn(report);\n            }\n        }, opts.timeout);\n    }\n\n    detectRender(ref, opts = {}) {\n        const ERROR = Detect.STATUS;\n        this.next(report => {\n            report(ref.offsetHeight ? ERROR.SUCCESS : ERROR.NOT_RENDER);\n        }, opts.timeout || 1000);\n    }\n}\n\nDetect.STATUS = {\n    SUCCESS: {status: 0},\n    NOT_UPDATE: {status: 1},\n    NO_DATA: {status: 2},\n    NOT_RENDER: {status: 3},\n    NO_CONTAINER: {status: 4},\n    MASKING: {status: 5},\n    REVERSAL: {status: 6}\n};\n\nexport default Detect;\n\n\n\n// WEBPACK FOOTER //\n// ./node_modules/@super-fe/BdrainfnDetect/src/index.js","var isArray = require('./isArray'),\n    isKey = require('./_isKey'),\n    stringToPath = require('./_stringToPath'),\n    toString = require('./toString');\n\n/**\n * Casts `value` to a path array if it's not one.\n *\n * @private\n * @param {*} value The value to inspect.\n * @param {Object} [object] The object to query keys on.\n * @returns {Array} Returns the cast property path array.\n */\n